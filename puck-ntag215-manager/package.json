    "@types/bootstrap": "^3.4.0",
    "@types/jquery": "^3.5.32",
    "@types/jszip": "^3.4.1",
    "@types/node": "^22.15.29",
    "@types/web-bluetooth": "0.0.21",
    "acorn": "^5.4.1",
    "arraybuffer-loader": "^1.0.8",
